<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PersianUIControlsMaui.Controls.TreeView"
             xmlns:conv="clr-namespace:PersianUIControlsMaui.Converters"
             xmlns:controls="clr-namespace:PersianUIControlsMaui.Controls"
             x:Name="treeView">
    <ContentView.Resources>
        <ResourceDictionary>
            <conv:FetchPropertyValueConverter x:Key="FetchPropertyValue"/>
            <conv:FilterbyPropertyConverter x:Key="FilterbyProperty"/>
        </ResourceDictionary>
    </ContentView.Resources>
    <VerticalStackLayout x:Name="items" Spacing="5"/>
</ContentView>